<!DOCTYPE HTML>

<html>
	<head>
		<title>{{ titre }}</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="themes/{{ theme }}/assets/css/main.css" />
		<noscript><link rel="stylesheet" href="themes/{{ theme }}/assets/css/noscript.css" /></noscript>
	</head>
	<body class="is-preload">

		<!-- Wrapper -->
			<div id="wrapper" class="fade-in">

				<!-- Intro -->
					<div id="intro">
						<h1>{{ titre }}</h1>
						<p>{{ sous-titre }}</p>
						<ul class="actions">
							<li><a href="#header" class="button icon solid solo fa-arrow-down scrolly">Continue</a></li>
						</ul>
					</div>

				<!-- Header -->
					<header id="header">
						<p class="logo">{{ titre-court }}</p>
					</header>

				<!-- Nav -->
					<nav id="nav">
						<ul class="links">
							{{ nav }}
						</ul>
						<ul class="icons">
							<li><a href="#" class="icon brands fa-twitter"><span class="label">Twitter</span></a></li>
							<li><a href="#" class="icon brands fa-facebook-f"><span class="label">Facebook</span></a></li>
							<li><a href="#" class="icon brands fa-instagram"><span class="label">Instagram</span></a></li>
							<li><a href="#" class="icon brands fa-github"><span class="label">GitHub</span></a></li>
						</ul>
					</nav>

				<!-- Main -->
					<div id="main">
                        <h1>Le contenu de mon template catalogue</h1>
						{{ contenu }}
					</div>

				<!-- Footer -->
					<footer id="footer">
						<section>
							{{ pied-page }}
						</section>
						<section class="split contact">
							{{ pied-infos }}
							<section>
								<h3>Social</h3>
								<ul class="icons alt">
									<li><a href="#" class="icon brands alt fa-twitter"><span class="label">Twitter</span></a></li>
									<li><a href="#" class="icon brands alt fa-facebook-f"><span class="label">Facebook</span></a></li>
									<li><a href="#" class="icon brands alt fa-instagram"><span class="label">Instagram</span></a></li>
									<li><a href="#" class="icon brands alt fa-github"><span class="label">GitHub</span></a></li>
								</ul>
							</section>
						</section>
					</footer>

				<!-- Copyright -->
					<div id="copyright">
						<ul><li>&copy; {{ author }}</li><li>Design: <a href="https://html5up.net">HTML5 UP</a></li></ul>
					</div>

			</div>

		<!-- Scripts -->
			<script src="themes/{{ theme }}/assets/js/jquery.min.js"></script>
			<script src="themes/{{ theme }}/assets/js/jquery.scrollex.min.js"></script>
			<script src="themes/{{ theme }}/assets/js/jquery.scrolly.min.js"></script>
			<script src="themes/{{ theme }}/assets/js/browser.min.js"></script>
			<script src="themes/{{ theme }}/assets/js/breakpoints.min.js"></script>
			<script src="themes/{{ theme }}/assets/js/util.js"></script>
			<script src="themes/{{ theme }}/assets/js/main.js"></script>

	</body>
</html>